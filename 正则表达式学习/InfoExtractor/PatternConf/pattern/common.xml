<?xml version="1.0" encoding="UTF-8"?>
<patterns>
<!-- unicode中的字符区间 -->
  <pattern id="CJK_Base" regexpr="\x{4E00}-\x{9FCB}"/>  <!--\u4E00-\u9FCB-->
  <pattern id="CJK_ExtA" regexpr="\x{3400}-\x{4DB5}"/>
  <pattern id="CJK_PUA"  regexpr="\x{E815}-\x{E864}"/>
  <pattern id="CJK_Add1" regexpr="\x{F900}-\x{FAD9}"/>
  <pattern id="CJK_关注" regexpr="〇\#CJK_ExtA\#\#CJK_Base\#\#CJK_PUA\#\#CJK_Add1\#"/>
  <pattern id="带圈数"   regexpr="\x{2460}-\x{2469}"/>
  <pattern id="带扩数"   regexpr="\x{2474}-\x{2487}"/>
  <pattern id="带点数"   regexpr="\x{2488}-\x{249B}"/>
  <pattern id="带扩数字" regexpr="\x{3220}-\x{3229}"/>

<!-- 预处理用各种日期、时间变种写法 -->
  <pattern id="数字零" regexpr="零〇○OoＯｏΟοОо"/>
  <pattern id="世纪数" regexpr="(?:[1一壹][8八捌9九玖]|[2二贰][0\#数字零\#一壹])"/>
  <pattern id="年份数" regexpr="[\d一壹二贰三叁四肆五伍六陆七柒八捌九玖\#数字零\#]"/>
  <pattern id="其它数" regexpr="[\d一壹二贰三叁四肆五伍六陆七柒八捌九玖十拾\#数字零\#]{1,3}"/>
  <pattern id="年月日" regexpr="(?&lt;=\n)((?:[1一壹][8八捌9九玖]|[2二贰][?0零〇○OoＯｏΟοОо０一壹])?[\d一壹二贰三叁四肆五伍六陆七柒八捌九玖?零〇○OoＯｏΟοОо０]{2})?年([\d一壹二贰三叁四肆五伍六陆七柒八捌九玖十拾零〇○OoＯｏΟοОо０]{1,3})?月([\d一壹二贰三叁四肆五伍六陆七柒八捌九玖十拾零〇○OoＯｏΟοОо０]{1,3})?日(?=\n)"/>
  <pattern id="年月" regexpr="(?&lt;=\n)(((?:[1一壹][8八捌9九玖]|[2二贰][?0零〇○OoＯｏΟοОо０一壹])?[\d一壹二贰三叁四肆五伍六陆七柒八捌九玖?零〇○OoＯｏΟοОо０]{2})?年([\d一壹二贰三叁四肆五伍六陆七柒八捌九玖十拾零〇○OoＯｏΟοОо０]{1,3})?月份?)(?!([\d一壹二贰三叁四肆五伍六陆七柒八捌九玖十拾零〇○OoＯｏΟοОо０]{1,3})?日)(?=\n)"/>

<!-- 汉语表达 -->
  <pattern id="无效字符"    regexpr="[\x{0000}-\x{0008}\x{000C}-\x{001F}\x{003F}\x{00A0}\x{3013}\x{E000}-\x{E7C6}\x{E7C9}-\x{E7E6}\x{E7F4}-\x{E814}\x{E865}-\x{F8FF}\x{FFFD}]"/>
  <pattern id="左括"        regexpr="（"/>
  <pattern id="右括"        regexpr="）"/>
  <pattern id="汉字"        regexpr="[\#CJK_关注\#]"/>
  <pattern id="中英文"      regexpr="[A-Za-z\#CJK_关注\#]"/>
  <pattern id="字中点"      regexpr="[·\#CJK_关注\#]"/>
  <pattern id="字顿"        regexpr="[、\#CJK_关注\#]"/>
  <pattern id="字括"        regexpr="[\#CJK_关注\#\#左括\#\#右括\#]"/>
  <pattern id="字顿括"      regexpr="[、\#CJK_关注\#\#左括\#\#右括\#]"/>
  <pattern id="字或数"      regexpr="[\d\#CJK_关注\#]"/>
  <pattern id="非【人】"    regexpr="[\#汉字\#&amp;&amp;[^人]]"/>
  <pattern id="非【审】"    regexpr="[^审\x{0000}-\x{4DFF}\x{9FA6}]"/>
  <pattern id="非【院】"    regexpr="[\#汉字\#&amp;&amp;[^院]]"/>
  <pattern id="非【与及】"  regexpr="[\#汉字\#&amp;&amp;[^与及]]"/>
  <pattern id="标点"        regexpr="[\s、，：。×∵∴∷]"/>
  <pattern id="标对左"      regexpr="[（〈〔［]"/>
  <pattern id="标对右"      regexpr="[）〉〕］]"/>
  <pattern id="非标点"      regexpr="[^\s、，：。×∵∴∷]"/>
  <pattern id="短句"        regexpr="\#非标点\#{2,}"/>
  <pattern id="中文数字"    regexpr="[〇零一二三四五六七八九十百千万亿]"/>
  <pattern id="列表数字"    regexpr="[〇零一二三四五六七八九十百\#带圈数\#\#带扩数\#\#带点数\#\#带扩数字\#]"/>
  <pattern id="数值"        regexpr="[\d〇零一二三四五六七八九十百千万亿]+"/>
  <pattern id="数额"        regexpr="\d*(?:\.\d+)?"/>
  <pattern id="列表序号"    regexpr="\#左括\#?(?:\d{1,3}?|\#列表数字\#{1,3}?)[\#右括\#、]"/>
  <pattern id="完整日期"    regexpr="((?:1[89]|20)?\d{2}年[01]?\d月[0-3]?\d日)"/>
  <pattern id="日期"        regexpr="((?:(?:1[89]|20)?\d\d年|[01]?\d月|\d?\d日){1,3})"/>
  <pattern id="期限"        regexpr="((?:\#数值\#年)?零?(?:\#数值\#个?月)?(?:\#数值\#[日天])?)"/>
  <pattern id="亲属"        regexpr="([父母]亲?|(曾?祖|[继岳])?[父母]|[玄曾外]孙[子女媳]|(?:丈?夫|妻子?)|[长次]?[子女孙兄嫂]|[大小二三四五六]([儿孙][子媳]|女儿?)|[二三四五六七八九十]?(爷爷|奶奶|姥[姥爷]|外婆|婆婆|伯[伯父母]?|[堂表]?(兄|哥哥?|姐[姐夫]?|弟[弟妹媳]?|妹[妹夫]?|叔[叔父母]?|舅[舅爷母妈]?|姑[姑父丈]?|姨[姨父丈]?|婶[婶子]?|嫂子?)))"/>
  <pattern id="行政区划"    regexpr="(?:自治[区州县旗]|特别行政区|[省市盟旗区县])"/>
  <pattern id="地址模板"    regexpr="(?:[^\s：，。\#左括\#\#右括\#]|（(?!户[口籍]|以下简称)[^\s：，。\#左括\#\#右括\#]+?）)"/>
  <pattern id="括号内说明"  regexpr="(?:\#左括\#(.+?)\#右括\#)"/>
  <pattern id="任职"        regexpr="(?:(?:捕前)?系|原[系任]?|\#汉字\#*?曾任)"/>

<!-- 同义表达词句 -->
  <pattern id="中国"     regexpr="(?:中国|中华人民共和国)"/>
  <pattern id="提供"     regexpr="(?:出具|出示|提供|提交|调取)"/>
  <pattern id="如上"     regexpr="(?:以上|上述|如上|上列)"/>
  <pattern id="如下"     regexpr="(?:下列|以下|如下)"/>
  <pattern id="如上事实" regexpr="(?:认定)?\#如上\#事实"/>
  <pattern id="如下证据" regexpr="(?:[还尚]?有(?:\#如下\#?[、，\x{4E00}-\x{9FA5}]*?证据)?(?:(?:予以)?证实)?|的证据(?:[还尚]?有|\#如下\#))"/>
  <pattern id="本院认为" regexpr="(?:本院|(?:经过?(?:复核)?审[查理]，|本案经过?)本院|本院经过?(?:复核)?审[查理]，?)认为"/>
  <pattern id="采纳意见" regexpr="[^。不]*?予以?采纳"/>
  <pattern id="以下简称" regexpr="(?:下称|简称|以下(?:简称|称[作为]?))"/>

<!-- 特殊范围 -->
  <pattern id="范围_全文"   regexpr="[\s\S]*"/>
  <pattern id="范围_无效尾" regexpr="\n//={50,}[\s\S]*"/>
  <pattern id="范围_前几行" regexpr="^(?:.*\n){0,5}?"/>
  <pattern id="范围_末几行" regexpr="(?:\n.*){0,100}?(?=\z)"/>
  <pattern id="范围_证据"   regexpr="\#如下证据\#：\n[\s\S]+?(?=\n\#本院认为\#)"/>

<!-- 名称类 -->
  <pattern id="人名"         regexpr="([A-Za-z]+(?:\x{0020}[A-Za-z]+){0,3}|\#汉字\#{1,6}(?:·\#汉字\#{1,6}){0,3})"/>
  <pattern id="多个人名"     regexpr="(\#人名\#(?:(?:[、和与及]|以及|伙同|联[合手]|串通|勾结)\#人名\#)*(?:等\#数值\#人)?)"/>
  <pattern id="法院名称"     regexpr="\#汉字\#{2,20}?\#非【审】\#法院(?:\#汉字\#{1,5}?分院)?"/>
  <pattern id="检院名称"     regexpr="\#汉字\#{2,20}?检察院(?:\#汉字\#{1,10}?分院)?"/>
  <pattern id="检察院名称" regexpr="\#汉字\#{2,20}?检察院(?:\#汉字\#{0,6}?分院)?"/>
  <pattern id="公安机关"     regexpr="((?:\#非【与及】\#+?[国省市区县])+公安[部厅局](?:\#非【与及】\#+?(?:分局|派出所|[大中总支]队))*)"/>
  <pattern id="案件类别简称" regexpr="[刑民经知行赔执]"/>
  <pattern id="字号"         regexpr="(?:刑|[民经知行赔执](?!.{0,4}?[复核])).{0,4}?([再抗]?[初终]|[监提再抗复核特督催审辖])字?"/>
  <pattern id="院称"         regexpr="\#汉字\#{0,4}?"/>
  <pattern id="案号"         regexpr="((?:\d{4}年?|\#标对左\#\d{4}\#标对右\#)\#院称\#\#字号\#.{1,12}?[号）])"/>
  <!-- <pattern id="案号_检"      regexpr="(([^\n]{1,4})检(?:\.?分)?(?:.{1,10})([\[(（［〔【]\d{4}[\])）］】〕]|\d{4})年?第?[\[(（［〔【]?(?:(?:\d{1,4}[－\-—])|(?:\d{1,4}、)+)?\d{1,4}[\])）］】〕]?[号）])"/> -->
  <pattern id="案号_检"      regexpr="(([^\n]{1,4})[检院](?:\.?分)?(?:.{1,10})([\[(（［〔【]\d{4}[\])）］】〕]|\d{4})年?第?[\[(（［〔【]?(?:(?:\d{1,4}[－\-—])|(?:\d{1,4}、)+)?\d{1,4}[\])）］】〕]?[号）])"/>
  <pattern id="案号_检2"      regexpr="(([\[(（［〔【]\d{4}[\])）］】〕]|\d{4})((.{1,4})检(?:\.?分)?(?:.{1,10})第?[\[(（［〔【]?(?:(?:\d{1,4}[－\-—])|(?:\d{1,4}、)+)?\d{1,4}[\])）］】〕]?[号）]))"/>
  <pattern id="法院裁判"     regexpr="((?:[刑民]事|行政)((?:附带民(?:事(?:诉讼)?|诉))?(判决|裁定|调解)))"/>
  <pattern id="审查逮捕案件" regexpr="(?:审查逮捕(?:案件)?意见)书" />
  <pattern id="审查起诉案件" regexpr="(?:(?:刑事附带民事|补充)?起诉|不起诉(?:决定)?|公诉)书" />
  <pattern id="反贪案件" regexpr="\#中英文\#{0,5}案侦查计划|[询讯]问笔录|(?:侦查终结|初查结论|提请初查|(?:提请|补充)立案)报告|(?:报请逮捕|(?:起诉|报送案件|报请重新审查逮捕)意见)书" />
  <pattern id="上诉及审判监督案件" regexpr="(?:(?:(?:刑事|民事|行政)抗诉)|(?:(?:(?:民事|行政|民事行政)检察)?提请抗诉报告)|刑事申诉复查决定)书" />
  <pattern id="检察文书"     regexpr="(?:(?:\#审查起诉案件\#)|(?:\#审查逮捕案件\#)|(?:\#上诉及审判监督案件\#)|(?:\#反贪案件\#))"/>
  <pattern id="法院文书"     regexpr="(刑事(?:附带民(?:事(?:诉讼)?|诉))?|民事|行政|赔偿|执行)((?:赔偿)?(判决|裁定|调解)书)"/>
  <pattern id="审判人员角色" regexpr="((?:[代助]理)?审判[长员]|(?:助审|代?书记|(?:人民)?陪审)员|代?院长)"/>
  <pattern id="合议庭角色"   regexpr="((?:[代助]理)?审判[长员]|(?:助审|(?:人民)?陪审)员|代?院长)"/>
  <pattern id="检察员"       regexpr="((?:[代助]理)?检察员|代?书记员|副?检察长|承办人)"/>
  <pattern id="检察员(不含书记员)" regexpr="((?:[代助]理)?检察员|副?检察长|承办人)"/>
  <pattern id="书记员角色" regexpr="(?:(?:(?:代|代理)?(书记|速录))员)[：:]?"/>
  
  
<!-- 数据特征 -->
  <pattern id="别名"     regexpr="(?:曾用|英文|[化别又乳网])名：?\#人名\#"/>
  <pattern id="绰号"     regexpr="[绰外]号：?((?:“?\#人名\#”?、)*“?\#人名\#”?)"/>
  <pattern id="国籍"     regexpr="(?&lt;=[、，。])(\#字括\#+?)(国?籍|公民)(?=[、，。])"/>
  <pattern id="民族"     regexpr="(\#汉字\#{1,6}?族)"/>
  <pattern id="文化程度" regexpr="(?:(?:小学|大学(?:[专本]科)?|[初高]中|中[学技师]|职高|职业高中|[中大]专|(?:在[校读])?(?:[硕博]士)?研究生|博士后)(?:[毕肄]业|(?:[一二三四五六七八九]年[半级]?)?)?)(?:文化)?(?:程度)?|半?文盲"/>
  <pattern id="地址"     regexpr="(?:[国省州市盟旗区县乡镇沟村庄屯队街道路里条巷组排号座栋楼层门室舍校园苑宫府场厂]|[大中小]学|大[院厦]|宿舍|公寓|院内?|单元|胡同)"/>
  <pattern id="身份"     regexpr="(无业(?:人员)?|[居农村渔牧]民|学生|(?:退休|下岗)?(?:工人|职[工员])|[离退]休干部|(?:退[役伍]|转业)?军[官人]|个体)"/>
  <pattern id="单位"     regexpr="(?:中心|机关|学校|[院所部厅司局处科室园店厂矿场馆社会站台行狱庭组团寺祠庵宫堂网]|[大中小]学|[酒氧网翅]吧|(?:[酒茶影大]|百货大?)楼|[商大]厦|(?:[茶鞋影]|购物|[家傢][具俱私俬]|[装灯]饰)城|超市|超级市场|(?:面包|[恋练]歌|球)房|(?:[发饼]|咖啡)屋|公墓)"/>
  <pattern id="政治面貌" regexpr="((?:(?:(中共(?:预备)?)|农工|致公)党|共青团|九三学社社|民[革建进]会|[民台]盟盟)员)"/>
  <pattern id="律师单位" regexpr="([^，。：（）]+?(?:[分务]所|中心|处))(?:实习)?律师"/>
  <pattern id="Ｘ长们"   regexpr="副?(?:[院校所厂矿场馆台园工社会部厅司局处科组省市区县乡镇村屯行署段站庭课股团队船舰艇车]|[董理监干]事|[司事教]务|[检监]察|审[判计]|裁判|秘书|委员|稽核|仲裁|护士|厨师|[监典]?狱|轮?机|水手)长"/>
  <pattern id="Ｘ官们"   regexpr="(?:首席)?(?:监[察督誓]|执[行事]|财[政务]|信息|技术|联络|协调|检察|大?法)官"/>
  <pattern id="Ｘ员们"   regexpr="(?:(?:[官委科社会成教学团队职雇店柜演艺文随海船警]|(?:助理)?审判|[陪助]审|书记|检[查察]|公证|仲裁|[观侦视]察|监[督理察]|[观检监]测|质[检保]|研究|[办炊]事|[服业公法乘电司事总机站票话勤]务|运动|(?:[足篮排棒垒台手冰]|橄榄|乒乓|曲棍|沙壶)球|[安协仓库房物车专]管|[撰校]稿|协[调办理]|[投邮]递|治安|[安消]防|[实试]验|[训教辅指]导|安[全检]|技术|通讯|机械|摄像|广播|播音|讲解|[材配资]料|[教训]练|巡[视逻边]|驾驶|[导收采]购|(?:市场)?推[广销]|咨询|营[业销]|[估造]价|销售|促销|操作|信[息访贷]|宣[传讲]|售[货楼票]|编[码目]|[管助]理|出纳|追款|[预结清核]算|保[管洁险健卫育]|养护|饲养|验[收票]|收[款银费料发购税]|护场|理[赔货]|定损|勘[察查]|查勘|[审稽][核查]|统计|演奏|(?:[高中初]级)?程序|(?:系统)?(?:分析|设计)|测[量试]|打字|校对|培训|劳资|司票|调[解度研查酒剂]|维修|报[关检告道导幕]|作业|[制描绘]图|宇航|航天|飞行|情报|审计|评[议论审判]|拍卖|列车|值[班勤机])|保安员?|专务|\#汉字\#{0,3}?[专人]员)"/>
  <pattern id="Ｘ师们"   regexpr="(?:[老教讲导律厨技禅医药琴乐]|美[发甲容术]|[设统审会]计|调[酒香音]|[精算预]算|[策规企]划|[茶园农]艺|测[量评]|建[筑造]|面[包点]|摄[像影]|驯[马兽]|造[价型]|验[光房]|营[销养]|[商税]务|按摩|保健|簿记|裁缝|采购|催眠|道具|雕刻|动画|发型|服装|工程|化妆|绘图|机械|架构|监理|鉴定|经济|理财|理发|录音|魔术|拍卖|培训|烹调|评估|气功|物流|演奏|药剂|阴阳|育婴|针灸|整容|政工|注册|咨询)师"/>
  <pattern id="Ｘ家们"   regexpr="\#汉字\#{0,8}?(?:[专作学]|科学|理论|政治|军事|艺术|书法|歌唱|舞蹈|文学|[演小]说)家"/>
  <pattern id="Ｘ工们"   regexpr="(?:[职员佣散杂小月替民美技矿窑牧渔绣车钳铣刨磨铸焊铀锉锯铆钻木土石瓦陶砖水泥漆油篆篾]|采[矿石油]|装[配卸修]|水[管泥暖]|[扳管巡]道|检[修验]|养[护路]|印[染刷]|织[布造]|煤[化矿]|[电铆]焊|[机维]修|[安包]装|[铺筑]路|[保清]洁|[送泥]水|[勤零]杂|[裁砖]石|[烫熨]衣|[处修]理|搬运|帮厨|包身|玻璃|操作|车床|刺绣|搓澡|挡车|雕刻|调试|翻砂|纺纱|缝纫|钢筋|焊接|合同|线路|小时|信号|学徒|冶炼|造纸|榨油|针织|制造|钟点|钻井|缫丝|勘探|炉前|磨粉|抹灰|木雕|碾米|捻线|农电|排版|烧结|试用|临时|暑期|树组|司炉|陶瓷|挑山)工"/>
  <pattern id="经理们"   regexpr="(?:(?:常务)?(?:[副总]|副总)|(?:客户|服务|售[前后]){1,2}|[企策]划|[财业]务|[研开]发|测试|质量|公关|人事|市场|广告|产品|项目|采购|营销|销售|大堂|基金)?经理(?:助理)?"/>
  <pattern id="总监们"   regexpr="(?:[法业财服常厂]务|[技艺美]术|营[运销业]|商[品务业]|[发运执]行|[企策]划|[研开]发|运[营作]|传[播媒]|媒[介体]|生产|销售|创意|设计|市场|工程|行政|医药|产品|项目|制作|音乐|舞台|行业|咨询|法律(?:事务)?|人力资源|人事)?部?副?总监"/>
  <pattern id="人员们"   regexpr="(?:工作|[财法服业]务|[开研]发|设计|销售|售[前后]|推[广销]|营[销运])人员"/>
  <pattern id="它务"     regexpr="(?:法定代表人|法人代表|副?总?书记|副?(?:教授|总裁|主[任管])|[董理监干]事|股东|会计|出纳|销售|秘书|司机|法务|顾问|(?:退休|下岗)?(?:工人|职工)|(?:[离退]休)?(?:政工)?干部)"/>
  <pattern id="职业"     regexpr="(无业(?:人员)?|农民|(?:[军艺]|(?:职业)?经理|(?:下岗)?工)人|(?:[演艺]|运动)员|作家|(?:(?:新闻)?媒体|法律)?(?:工作|记)者|警察|[交刑民法]警|\#Ｘ师们\#)"/>
  <pattern id="职务"     regexpr="(\#Ｘ长们\#|\#Ｘ官们\#|\#Ｘ员们\#|\#Ｘ师们\#|\#Ｘ家们\#|\#Ｘ工们\#|\#经理们\#|\#总监们\#|\#人员们\#|\#它务\#)"/>
  <pattern id="单位职务人名"  regexpr="([^\s、，。]+?\#单位\#\#括号内说明\#?的?\#职务\#\#人名\#)"/>
  <pattern id="特殊生病理"    regexpr="((?:又?聋又?哑|[^文\s]盲)人?)"/>
  <pattern id="强制措施种类"  regexpr="(监视居住|(?:刑事|行政)?拘留|取保候审|羁押|(?:执行)?逮捕|[押解]回审查)"/>
  <pattern id="主刑种类"      regexpr="(死刑|[无有]期徒刑|拘役|管制)"/>
  <pattern id="有刑期主刑"    regexpr="(有期徒刑|拘役|管制)"/>
  <pattern id="附加刑种"      regexpr="(罚金|没收其?(?:个人)?(?:全部|部分)?财产|剥夺政治权利|驱逐出境)"/>
  <pattern id="财产刑种"      regexpr="(罚金|没收(?:个人)?(?:全部)?财产)"/>
  <pattern id="货币种类"      regexpr="((?:人民|[新港台])币|(?:[欧美日韩]|加(?:拿大)?|新(?:加坡)?|澳(?:大利亚)?)[圆元]|英镑|(?:瑞士|法国|比利时)?法郎|(?:意大利)?里拉|卢布|(?:德国)?马克)"/>
  <pattern id="收费种类"      regexpr="((?:(?:[一二再]审|上诉)?(?:案件)?|其他)?(受理|诉讼|(?:诉[前讼])?(?:财产)?保全|(?:伤残)?鉴定)费)用?"/>

<!-- 典型句式 -->
  <pattern id="句_亲属"       regexpr="[系（](?:\#汉字\#*?(?:[原被]告|被害|[自上]诉)人?)?(.*?)[的之]\#亲属\#[，。）]"/>
  <pattern id="句_律师及单位" regexpr="(?:([^\s，。：（）]+?(?:[分务]所|中心|处))((?:实习)?律师(?:助理)?|法律工作者))"/>
  <pattern id="句_指定管辖"   regexpr="(?:(?:[遵依]照|[根依]据)\#法院名称\#指[定令]管辖的?决定|，指[定令][我本]院(?:管辖|立案|[审受]理)|\#法院名称\#(?:决定，指[定令][我本]院(?:管辖|[审受]理)|指[定令]管辖，[我本]院).{4}(?:立案|管辖|[审受]理))"/>

<!-- 【法律依据】相关 -->
  <pattern id="法律依据"      regexpr="((?:最高人民法院\#字顿\#*?关于(?:[处审]理|适用|执行))?《.+?》(?:.*?解释)?)"/>
  <pattern id="连写法律条款"  regexpr="(?:[第〇零一二三四五六七八九十百条款项（）]{3,}?(?:[、，和与及]|以及|[之的]规定))+"/>
  <pattern id="单一法律条文"  regexpr="(第?\#中文数字\#{1,5}?条([^条]+?[款项])*)(?=[\n、，和与及]|以及|[之的]规定)"/>  
  <pattern id="多个罪名"  		regexpr="(?:[^罪]犯|构成了?|涉嫌犯?|以)([\x{4E00}-\x{9FA5}、]+?(?:、[^罪\x{0000}-\x{4DFF}\x{9FA6}]+)*?罪(?:[、和与及][\x{4E00}-\x{9FA5}（）]+(?:、[^罪\x{0000}-\x{4DFF}\x{9FA6}]+)*?罪)*)[^犯行活]"/>

<!-- 一些段落的结尾标记，仅用于定位，不纳入捕获组 -->
  <pattern id="终结关键字"    regexpr="(?:审[理查核]|复核)(?:终结|完[结毕])"/>
  <pattern id="受理关键字"    regexpr="(?:发生纠纷|提[起出](?:[公控上申抗]诉|诉讼)|(?:指控|审理)[^\s。]+?一案|[诉辩]称|诉至[本法]院|起诉来院|要求[和与]?被告|(?:本院在?|立案)[受审]理|合议庭|独任审判|适用简易程序|\#终结关键字\#)"/>
  <pattern id="段前_当事人"   regexpr="^(?:.*\n){0,3}?(?:(?:\#检院名称\#)?\#检察文书\#.{0,20}|\#案号_检\#|\#法院名称\#[:：]?)(?=\n)"/>
  <pattern id="段后_当事人"   regexpr="(?=\n.*?(?:案由：|\#受理关键字\#))"/>
  <pattern id="段后_当事人_起诉"   regexpr="(?=\n.*?(?:侦查终结|审查起诉|(?:。|经(?:本院)?依法审查，?)现?查明)|(?:认定上述事实的证据|上述事实)|(?:本院认为)|(?:决定对[^，。]+?不起诉))"/>

<!-- 段落识别 -->
  <!-- 多种审级通用 -->
  <pattern id="段_检院名称"  regexpr="\#范围_前几行\#(\#检院名称\#)(?=\n)"/>
  <pattern id="段_法院名称"  regexpr="\#范围_前几行\#(\#法院名称\#)"/>
  <pattern id="段_文书名称"  regexpr="\#范围_前几行\#(?:\#检院名称\#)?(\#检察文书\#).{0,20}\n"/>
  <pattern id="段_文书名称2" regexpr="\#范围_无效尾\#\n(?:标题：?)?(?:\#检院名称\#)?(\#检察文书\#).{0,20}(?=\n|\z)"/>
  <pattern id="段_案号"      regexpr="\#范围_前几行\#\#案号\#\n"/>
  <pattern id="段_案号_检"   regexpr="\#范围_前几行\#\#案号_检\#\n"/>
  <pattern id="段_案号_检2"   regexpr="\#范围_前几行\#\#案号_检2\#\n"/>
  <pattern id="段_案号2"     regexpr="\#范围_末几行\#\n(?:发文字号：?)?\#案号\#(?=\n|\z)"/>

  <!-- [段_当事人]中 ([\s\S]+) 的捕获组为[5]，因[段前_当事人]中的[法院文书]、[案号]分别有2个分组-->
  <pattern id="段_当事人"    regexpr="\#段前_当事人\#([\s\S]+?)\#段后_当事人\#"/>
  <pattern id="段_事实"      regexpr="审理查明[：，]([\s\S]+)\#本院认为\#[：，]"/>
  <pattern id="裁判"         regexpr="(?:缺席(?:判决|裁定)|(?:判决|裁定)如下)"/>
  <pattern id="段_裁判结果"  regexpr="\#裁判\#[：，。]?(?![\s\S]+\#裁判\#)([\s\S]+?)(\n.*?报请最高人民法院核准[\s\S]*?|\n.{1,4}?年.{0,2}?月.{0,2}?日[\s\S]*?)?(?=\z)"/>
  <pattern id="段_调解结果"  regexpr="(?:[如以]下(?:调解)?协议|(?:调解)?协议如下)：(\n[\s\S]+?\n)(.+?年.*?月.*?日)?\#范围_末几行\#"/>
  <pattern id="段_案件受理"  regexpr="(?:\n案由：.+)?\n.*?\#受理关键字\#.*\n"/>
  
  <pattern id="起诉书_段_当事人"    regexpr="\#检察文书\#(?:\#段_案号_检\#)?([\s\S]+?)\#段后_当事人_起诉\#"/>

<!-- 逻辑行(记录)识别 -->
  <pattern id="段_原审信息"  regexpr="[原一]审法院认为.+?判决：((.*\n)+?).*?[上抗]诉.*?[，。：]"/>
  <pattern id="行_案前诉讼"  regexpr="(?:[^。\s]+?(?:指控|诉讼|[上起抗公控]诉))?(?:.*?于\#日期\#作出|.*?本院以|.*?不服(\#法院名称\#))?\#案号\#?.*?\#法院裁判\#(?:[^。\s]*?发回[^。\s]*?重新?审判?|[^。\s]*?驳回)?.*?。"/>
  <pattern id="行_单位任职"  regexpr="[、，。]\#任职\#?([^\s、，。]+?\#单位\#([^\s、，。]+?\#单位\#)*)(\#汉字\#{0,4}?\#职务\#(?:[兼、]\#汉字\#{0,4}?\#职务\#)*)(?=[、，。])"/>
  
  <pattern id="反贪文书_犯罪嫌疑人" regexpr="(?:^|\n)((犯罪嫌疑人[\s\S]+?(?=\n被|\n(以上|上[述列]).{1,2}(?:犯罪)?嫌疑人|$))|(?:[\d一二三四五六七八九十]+、)(?:犯罪嫌疑(?:人|单位)[：:（]?)[\s\S]*?(?=\n(?:[\d一二三四五六七八九十]+、)被|$))"/>
  
  <pattern id="反贪文书_线索来源" regexpr="((?:单位|公民)?举报|上级交办|移送|(?:办案中|自行)发现)"/>
</patterns>
