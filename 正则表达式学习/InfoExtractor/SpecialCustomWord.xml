	<!--在全文中提取 网络虚拟账号 -->
	<parser name="WLXNZH" nameCN="网络虚拟账号" keepValue="true">
		<searchers>
			<searcher>
				<!--pattern>((?i:qq|e\-?mail|skype|微信|微博|淘宝|邮箱)(?:号?码?|账号))[:：（(]?([\w@.]+(?:、[\w@.]+)*)</pattern-->
				<pattern>(?:(?:淘宝|支付宝|网站|微博)(?:号?码?|[账帐]号|[账帐]户|用?户名))[:：是为系（(]?([\w@.]+(?:、[\w@.]+)*)</pattern>
				<result>$1</result>
			</searcher>
			<searcher>
				<pattern>(?:(?:[账帐][号户]{1,2}|用?户名)[号码]+)[:：是为系（(]?([a-zA-Z][a-zA-Z\d\-._]*|(\w)+(\.\w+)*@(\w)+((\.\w+)+))</pattern>
				<result>$1</result>
			</searcher>
		</searchers>
	</parser>
	<parser name="WLXNTXFS" nameCN="网络通讯方式" keepValue="true">
		<searchers>
			<searcher>
				<pattern>(?:(?i:qq|e\-?mail|skype|msn|(?:电子)?邮箱)(?:号?码?|账号))[:：（(]?([\w@.]+(?:、[\w@.]+)*)</pattern>
				<result>$1</result>
			</searcher>
			<!--把微信单独拿出来是因为微信账号可以是很多字符 -->
			<searcher>
				<pattern>(?:微信(?:号?码?|[账帐]号))[是为系:：（(]?(?!聊天记录|照片)([a-zA-Z\d\-\x{4E00}-\x{9FA5}]+?)[,；.，。]</pattern>
				<result>$1</result>
			</searcher>
		</searchers>
	</parser>
	<!--  检查员姓名 -->
	<parser name="JCYNAME" nameCN="检察员姓名" keepValue="true">
		<searchers>
			<searcher>
				<pattern>(?:检\s*察\s*员|书\s*记\s*员)[:：\s]*((?:[A-Za-z·•]|[^等与的犯罪证（）\x{0000}-\x{4DFF}\x{9FA6}]|\s){2,}?)(?=代\s*理\s*检\s*察\s*员|书\s*记\s*员|检\s*察\s*员|\n)</pattern>
				<result>$1</result>
			</searcher>
			<searcher>
				<pattern>(?:检\s*察\s*员|书\s*记\s*员)[:：\s]*(?:[A-Za-z·•]|[^等与的犯罪证（）\x{0000}-\x{4DFF}\x{9FA6}]|\s){2,}?((?:[\r\n]+[a-zA-Z\x{4E00}-\x{9FA5} ]{2,5})+)(?=[\n\r])</pattern>
				<result>$1</result>
			</searcher>
		</searchers>
	</parser>
