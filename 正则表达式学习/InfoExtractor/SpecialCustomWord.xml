	<!--组织机构代码 -->
	<parser name="ZZJGDM" nameCN="组织机构代码" keepValue="true">
		<searchers>
			<searcher>
				<pattern>(?&lt;!\w)组织机构代码[为是系：:\x{0020}]*(\w{8}-?\w)(?!\w)</pattern>
				<result>$1</result>
			</searcher>
		</searchers>
